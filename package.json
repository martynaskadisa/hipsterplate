{
  "name": "hipsterplate",
  "version": "0.1.0",
  "description": "",
  "main": "./index.ts",
  "scripts": {
    "lint": "tslint \"./src/**/*.ts?(x)\"",
    "autofix": "tslint -e \"./src/**/*.d.ts\" --fix \"./src/**/*.ts?(x)\"",
    "test": "jest",
    "build": "rimraf dist && cross-env TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\"}\" webpack --config ./webpack/webpack.config.ts --progress --profile --colors --display-optimization-bailout",
    "start": "cross-env TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\"}\" ts-node -r tsconfig-paths/register ./index.ts",
    "debug": "cross-env TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\"}\" ts-node --inspect -r tsconfig-paths/register ./index.ts",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "./node_modules/.bin/tslint -e \"./src/**/*.d.ts\" --force --fix -t stylish",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "client/(.*)": "<rootDir>/src/client/$1",
      "^.+\\.s?css$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverage": true,
    "mapCoverage": true
  },
  "engines": {
    "node": ">7.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martynaskadisa/hipsterplate.git"
  },
  "author": "Martynas Kadi≈°a",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martynaskadisa/hipsterplate/issues"
  },
  "homepage": "https://github.com/martynaskadisa/hipsterplate#readme",
  "resolutions": {
    "@types/react": "16.1.0",
    "@types/react-dom": "16.0.4"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.4",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/extract-text-webpack-plugin": "^3.0.2",
    "@types/helmet": "^0.0.36",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/jest": "^22.1.2",
    "@types/morgan": "^1.7.35",
    "@types/raven": "^2.1.5",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-helmet": "^5.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.4",
    "@types/webpack": "^4.1.3",
    "@types/webpack-bundle-analyzer": "^2.9.2",
    "@types/webpack-dev-middleware": "^2.0.1",
    "@types/webpack-hot-middleware": "^2.16.3",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^8.0.0",
    "awesome-typescript-loader": "^3.4.1",
    "chalk": "^2.3.1",
    "chokidar": "^2.0.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "enzyme": "^3.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.3.0",
    "lint-staged": "^6.1.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.15.0",
    "offline-plugin": "^4.9.0",
    "postcss-loader": "^2.1.0",
    "prettier": "^1.10.2",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.0.4",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.8.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
