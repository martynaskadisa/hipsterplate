  },
  "dependencies": {
    "axios": "^0.17.1",
    "express": "^4.16.2",
    "helmet": "^3.11.0",
    "http-status-codes": "^1.3.0",
    "morgan": "^1.9.0",
    "raven": "^2.4.1",
    "raven-js": "^3.22.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-lazy-import": "^0.1.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
